<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oa.dao.StateDao">

	<select id="findAll" resultType="State">
		select * from state
	</select>

	<select id="findById" resultType="State">
		select * from state where id =
		#{id}
	</select>

</mapper>